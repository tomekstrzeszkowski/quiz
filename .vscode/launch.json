    "version": "0.2.0",
    "configurations": [{
        "name": "Listen",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${file}",
        "args": ["-l", "10000"]
    },{
        "name": "Connect",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceRoot}",
        "args": ["-l", "10001", "-d", "/ip4/127.0.0.1/tcp/10000/p2p/<connection-id-goes-here>"]
    }]
}